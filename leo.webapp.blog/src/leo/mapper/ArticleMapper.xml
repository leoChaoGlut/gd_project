<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="leo.mapper.ArticleMapper">
	<select id="selectMore" resultType="Article">
		SELECT
		*
		FROM
		blog_article
		LIMIT
		#{startIndex},#{pageSize}
	</select>
</mapper>